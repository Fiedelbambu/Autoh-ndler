                        <Insets bottom="5.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="button_scene3" mnemonicParsing="false" onAction="#handleButton3Action" text="Screen3">
                     <VBox.margin>
                        <Insets bottom="5.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="button_scene4" mnemonicParsing="false" onAction="#handleButton4Action" text="Screen4">
                     <VBox.margin>
                        <Insets bottom="5.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="button_scene5" mnemonicParsing="false" onAction="#handleButton5Action" text="Screen5">
                     <VBox.margin>
                        <Insets bottom="5.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
