   <children>
      <BorderPane fx:id="mainPane" layoutX="-2.0" layoutY="-1.0" prefHeight="400.0" prefWidth="600.0">
         <left>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: yellow;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="button_scene1" mnemonicParsing="false" onAction="#handleButton1Action" text="Screen1">
                     <VBox.margin>
                        <Insets bottom="5.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="button_scene2" mnemonicParsing="false" onAction="#handleButton2Action" text="Screen2">
